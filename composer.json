{
    "name"              : "corycollier/mvc-lite",
    "description"       : "A lightweight MVC Framework, inspired by Zend Framework",
    "license"           : "MIT",
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "homepage"          : "http://corycollier.github.io/mvc-lite/",
    "require-dev"       : {
        "phpunit/phpunit-mock-objects" : "^3.0",
        "squizlabs/php_codesniffer"    : "^2.3",
        "phpmd/phpmd"                  : "^2.3",
        "pdepend/pdepend"              : "^2.2",
        "phploc/phploc"                : "^2.1",
        "theseer/phpdox"               : "0.8.1.1",
        "phpunit/phpunit"              : "^5.0"
        "sebastian/phpcpd"             : "^2.0"
    },
    "authors" : [
        {
            "name"     : "Cory Collier",
            "email"    : "corycollier@corycollier.com",
            "role"     : "developer",
            "homepage" : "http://corycollier.com/"
        }
    ],
    "require" : {
        "corycollier/php-cli": "^1.0.0",
    },

    "autoload": {
        "psr-4": {
            "MvcLite\\": "src/",
            "App\\" : "./App"
        }
    }
}
